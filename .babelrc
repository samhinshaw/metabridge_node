{
  "presets": [
    "next/babel",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-class-properties"],
  "env": {
    "graphql": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
