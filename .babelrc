{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true,
        "modules": false
      }
    ],
    "next/babel"
  ],
  "plugins": ["transform-class-properties"],
  "env": {
    "graphql": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}
